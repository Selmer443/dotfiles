(defwidget unfocus-main[]
  (eventbox :class "unfocus-eventbox"
           :onclick "scripts/unfocus"))


(defwindow unfocus 
:monitor 0
:geometry (geometry :x "0px" :y "0px" :width "100%" :height "100%" :anchor "top left")
:windowtype "dock"
:wm-ignore false
(unfocus-main))
  
  
(defwindow unfocus 
  :monitor 1
  :geometry (geometry :x "0px" :y "0px" :width "100%" :height "100%" :anchor "top left")
  :windowtype "dock"
  :wm-ignore false
  (unfocus-main))

